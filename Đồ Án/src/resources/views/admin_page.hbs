

<div class="row mt-5">
  <div class="col-md-6 m-auto">
    <div class="card card-body">
     <h1 class="text-center mb-3"><i class="fas fa-sign-in-alt"></i>Trang thông tin tài khoản</h1>
        
         {{#each users}}
        <div class="form-group">
             <label for="email">Email</label>
             <h5>{{this.email}}</h5>
            </div>
            <div class="form-group">
             <label for="name">Name</label>
             
             <h5> {{this.name}} </h5>
            </div>
            <div class="form-group">
             <label for="password">Password</label>
            <h5> {{this.password}} </h5>
            </div>
            <a href="/admin_page/{{this._id}}">delete</a>
            <hr>
         {{/each}}
    
      
      </div>
  </div>
</div>
